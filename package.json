{
  "name": "gulpbs",
  "version": "0.0.2",
  "description": "Gulp-based boilerplate for new projects",
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "build": "./node_modules/.bin/gulp build",
    "prod": "./node_modules/.bin/gulp build --production",
    "dev": "./node_modules/.bin/gulp dev --development",
    "icoopen": "./node_modules/.bin/fontello-cli open --config ./frontend/styles/includes/fontello/config.json",
    "icogen": "./node_modules/.bin/fontello-cli install --config ./frontend/styles/includes/fontello/config.json --css ./frontend/styles/includes/fontello/css --font ./frontend/styles/includes/fontello/font"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vahrammer/gulpbs.git"
  },
  "author": "Sergey Vakhrameev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vahrammer/gulpbs/issues"
  },
  "homepage": "https://github.com/vahrammer/gulpbs#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "cssnano": "^4.1.5",
    "del": "^3.0.0",
    "fontello-cli": "^0.4.0",
    "gulp": "^4.0.0",
    "gulp-data": "^1.3.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "node-sass": "^4.9.4",
    "stream-combiner2": "^1.1.1",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.21.0",
    "webpack-stream": "^5.1.1",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.1",
    "slick-carousel": "^1.8.1",
    "underscore": "^1.9.1"
  }
}
